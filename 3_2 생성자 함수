/*
생성자 함수

- 객체 리터럴
let user = {
    name : 'Mike',
    age : 30,
}
- 생성자 함수
function User(name, age){  // User의 첫글자는 대문자로
    //this = {};
    this.name = name;
    this.age = age;
    this.sayName = function(){
        console.log(this.name);
    }
    //return this;
}

let user1 = new User('Mike', 30);
let user2 = new User('Jane', 22);
let user3 = new User('Tom', 17);
user3.sayName(); // 'Tom'

*/
